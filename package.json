{
  "name": "imageservice",
  "version": "1.0.0",
  "description": "http service that resizes an image with several resolutions then uploads everything to S3",
  "main": "server/src/index.js",
  "repository": "Sistemium/imageService",
  "dependencies": {
    "aws-sdk": "^2.686.0",
    "body-parser": "^1.19.0",
    "debug": "^2.6.9",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "gm": "^1.23.0",
    "image-size": "^0.8.3",
    "imagemagick": "^0.1.3",
    "lodash": "^4.17.15",
    "mkdirp": "^1.0.4",
    "multer": "^1.4.2",
    "uuid": "^1.4.2",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "blanket": "^1.1.7",
    "chai": "^3.2.0",
    "mocha": "^2.2.5",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0"
  },
  "scripts": {
    "serve": "nodemon --exec 'node -r esm' -- server/src",
    "test": "mocha && mocha test --require blanket --reporter html-cov > coverage.html"
  },
  "config": {
    "blanket": {
      "pattern": [
        "server/src/check-json-file-structure.js"
      ],
      "data-cover-never": "node_modules"
    }
  },
  "author": "Sistemium UAB <uab@sistemium.com>",
  "contributors": [
    {
      "name": "Alexander Levin",
      "email": "a@sistemium.com"
    },
    {
      "name": "Albert Kovalevskij"
    }
  ],
  "license": "ISC"
}
