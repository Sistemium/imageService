{
  "name": "imageservice",
  "version": "1.0.0",
  "description": "http service that resizes an image with several resolutions then uploads everything to S3",
  "main": "server/src/index.js",
  "repository": "Sistemium/imageService",
  "dependencies": {
    "aws-sdk": "^2.6.15",
    "body-parser": "^1.13.2",
    "debug": "^2.3.2",
    "express": "^4.14.0",
    "gm": "^1.23.0",
    "image-size": "^0.3.5",
    "imagemagick": "^0.1.3",
    "lodash": "^3.10.0",
    "mkdirp": "^0.5.1",
    "multer": "^0.1.8",
    "node-uuid": "^1.4.3",
    "q": "^1.4.1",
    "request": "^2.58.0"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "blanket": "^1.1.7",
    "chai": "^3.2.0",
    "mocha": "^2.2.5",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0"
  },
  "scripts": {
    "serve": "nodemon server/src/index.js",
    "test": "mocha && mocha test --require blanket --reporter html-cov > coverage.html"
  },
  "config": {
    "blanket": {
      "pattern": [
        "server/src/check-json-file-structure.js"
      ],
      "data-cover-never": "node_modules"
    }
  },
  "author": "Sistemium UAB <uab@sistemium.com>",
